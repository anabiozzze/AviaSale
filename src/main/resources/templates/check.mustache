                {{#clients}}
                <div>
                    <span style="color: #f1f1f1"> {{firstName}}</span>
                    <i style="color: #f1f1f1"> {{lastName}}</i>
                    <span style="color: #f1f1f1"> {{nationality}}</span>
                    <span style="color: #f1f1f1"> {{sex}}</span>
                    <span style="color: #f1f1f1"> {{birthDate}}</span>
                    <span style="color: #f1f1f1"> {{passNumber}}</span>
                </div>
                {{/clients}}


                <form method="get">
                <p class="btn"><button class="btn1" type="submit">CORRECT</button></p>
                </form>

